{
    "ignore:id": 1,
    "ignore:name": "time",
    "ignore:description": "The service in charge of gathering the time.",
    "service": {
        "ignore:id": 1,
        "input:url_extra": "/api/time/current/zone",
        "drop:url_params": {
            "opt:timeZone=": [
                "opt:Europe/Paris",
                "default:Europe/Berlin",
                "opt:Europe/Madrid",
                "opt:Europe/London"
            ],
            "input:additional_params": ""
        },
        "body": {
            "input:additional_body": ""
        },
        "drop:method": [
            "default:GET",
            "opt:POST",
            "opt:PUT",
            "opt:PATCH",
            "opt:DELETE",
            "opt:HEAD",
            "opt:OPTIONS"
        ],
        "header": {
            "input:additional_header": ""
        },
        "ignore:token_location": {
            "param": false,
            "header": false,
            "body": false,
            "bearer": false
        },
        "response": {
            "int:code": [
                "default:200",
                "min:100",
                "max:500"
            ],
            "input:response_content": "$ref{body.hour}:$ref{body.minute}"
        },
        "drop:verification_operator": [
            "default:==",
            "opt:===",
            "opt:eq",
            "opt:-eq",
            "opt:!=",
            "opt:<>",
            "opt:ne",
            "opt:-ne",
            "opt:<",
            "opt:lt",
            "opt:-lt",
            "opt:>",
            "opt:gt",
            "opt:-gt",
            "opt:<=",
            "opt:le",
            "opt:-le",
            "opt:>=",
            "opt:ge",
            "opt:-ge",
            "opt:<=>",
            "opt:equal to",
            "opt:less than",
            "opt:not equal to",
            "opt:greater than",
            "opt:less than or equal to",
            "opt:greater than or equal to"
        ],
        "time:verification_value": "9:00",
        "vars": {
            "current_time": "$ref{body.hour}:$ref{body.minute}:$ref{body.second}",
            "location": "Europe/Paris",
            "credits": "$ref{header.used.credits}"
        }
    }
}
